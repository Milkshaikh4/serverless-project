{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-use-before-define": 0,
        "func-names": 0,
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0, 
        "no-plusplus": 0, 
        "no-param-reassign": 0, 
        "implicit-arrow-linebreak": 0, 
        "function-paren-newline": 0
    }
}
